<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vender">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/vender_logo"
        android:label="@string/app_name"
        android:usesCleartextTraffic="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Activity.SplashActivity" android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Activity.HomeActivity" />
        <activity android:name=".Activity.CreateEventActivity"/>
        <activity android:name=".Activity.DetailMyEvent"/>
        <activity android:name=".Activity.RegisterActivity" />
        <activity android:name=".Activity.MyEventActivity"/>
        <activity android:name=".Activity.EditEventActivity"/>
        <activity android:name=".Activity.TalentRegisterActivity"/>
        <activity android:name=".Activity.MyTalentProfileActivity"/>
        <activity android:name=".Activity.DetailTalentActivity"/>
        <activity android:name=".Activity.EditTalentActivity"/>
        <activity android:name=".Activity.EditProfile"/>
        <activity android:name=".Activity.RatingActivity"/>
        <activity android:name=".Activity.MyJobActivity"/>
        <activity android:name=".Activity.DetailEventSubmission"/>
        <activity android:name=".Activity.DetailEventOffer"/>
        <activity android:name=".Activity.DetailEventList"/>
        <activity android:name=".Activity.DetailEventMyJob"/>
        <activity android:name=".Activity.WorkedTalentActivity"/>
        <activity android:name=".Activity.EventRelated"/>
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>